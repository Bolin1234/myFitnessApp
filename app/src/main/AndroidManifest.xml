<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.bolinwang.tudar">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".TrainerQuickAnswerRecycler">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SignUpInfo" />
        <activity android:name=".TrainerCertify" />
        <activity
            android:name=".TrainerMainActivity"
            android:label="@string/title_activity_trainer_main" />
        <activity
            android:name=".StudentMainActivity"
            android:label="@string/title_activity_student_main" />
        <activity android:name=".QuickAsk" />
        <activity android:name=".TrainerWaiting" />
        <activity android:name=".SignUpLogInActivity" />
        <activity android:name=".TrainerAnswerQuestionActivity"></activity>
    </application>

</manifest>